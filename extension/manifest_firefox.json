{
    "manifest_version": 3,
    "name": "Producer.ai Download Suite",
    "version": "3.2",
    "author": "ransomletter",
    "description": "Complete downloading toolkit: Bulk Tracks, Stems (Zip/Folder), and Organized Outputs.",
    "permissions": [
        "activeTab",
        "scripting",
        "downloads"
    ],
    "host_permissions": [
        "https://www.producer.ai/*"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "action": {
        "default_popup": "popup.html"
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "producer-suite@ransomletter.com",
            "strict_min_version": "109.0"
        }
    }
}